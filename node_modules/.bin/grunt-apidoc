#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules/grunt-apidoc/bin/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules/grunt-apidoc/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules/grunt-apidoc/bin/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules/grunt-apidoc/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/grunt-apidoc@0.11.0_grunt@1.6.1/node_modules:/mnt/d/Workspaces/Miscellaneous/backendPainter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../grunt-apidoc/bin/grunt-apidoc" "$@"
else
  exec node  "$basedir/../grunt-apidoc/bin/grunt-apidoc" "$@"
fi
